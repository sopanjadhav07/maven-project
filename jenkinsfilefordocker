node{
	        stage('SCM checkout')
			        {git 'https://github.com/sopanjadhav07/maven-project/new/master'}
	}
    
	
	
	
	 stage('package'){
					       sh 'mvn clean package'
						   }
					
    


    
     stage('docker image build')
	 {
                sh 'docker build -t sopanjadhav/maven-project .'
			}

pipeline{
    agentany{
	
	{
	stages{
	        stage('SCM checkout')
			        {git 'https://github.com/sopanjadhav07/maven-project/new/master'}
	}
    }	
	}
	
	
	{
	 stage('package')
	                 {
					 steps{
					       sh 'mvn clean package'
						   }
					}
    }


    {
     stage('docker image build')
     {
        steps{
                sh 'docker build -t sopanjadhav07/mydockimage:latest'
			}
    }
    }
}	

pipeline{
    agentany{
	
	{
	stages{
	        stage('SCM checkout')
			        {git 'https://github.com/sopanjadhav07/maven-project/new/master'}
	}
    }	
	}
	
	
	{
	 stage('package')
	                 {
					 steps{
					       sh label: '', script: 'mvn clean package'
						   }
					}
    }


    {
     stage('docker image build')
     {
        steps{
                sh label: '', script: 'docker build -t mydockimage/sopanjadhav07:latest'
			}
    }
    }
}	
